    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.11.2"
  },
  "version": "1.0.0",
  "description": "test (first) reactjs project",
  "main": "webpack.config.js",
  "repository": {
    "type": "git",
    "url": "none"
  },
  "license": "ISC"
}
